---
// Component Imports

import Navbar from '../components/Navbar.astro';
import Navbar2 from '../components/Navbar2.astro';
import PostsWrap from '../components/PostsWrap.jsx';


// You can import components from any supported Framework here!


// Component Script:
// You can write any JavaScript/TypeScript that you'd like here.
// It will run during the build, but never in the browser.
// All variables are available to use in the HTML template below.
let title = 'hot.opensauced.pizza';

// Full Astro Component Syntax:
// https://docs.astro.build/core-concepts/astro-components/
---
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <title>{title}</title>

        <link rel="icon" type="image/x-icon" href="/favicon.ico" />

    <link rel="stylesheet" href={Astro.resolve('../styles/global.css')}>
    <link rel="stylesheet" href={Astro.resolve('../styles/home.css')}>
</head>
<body>
<!-- page -->
    <main>
<Navbar />
<Navbar2 />
<PostsWrap client:load />
    </main>
</body>
</html>
